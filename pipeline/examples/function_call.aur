// Function call test
// Tests simple function definition and call

module math {
    fn add(a: int, b: int) -> int {
        let result: int = a + b;
        return result;
    }
    
    fn main() -> int {
        let x: int = 3;
        let y: int = 5;
        let sum: int = add(x, y);
        request service print(sum);
        request service exit(0);
        return 0;
    }
}
