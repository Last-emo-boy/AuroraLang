// Test basic math functions: abs, floor, ceil
// Expected output:
//   abs(-5.5) = 5.500000000
//   abs(3.14) = 3.140000000
//   floor(3.7) = 3.000000000
//   floor(-3.7) = -4.000000000
//   ceil(3.2) = 4.000000000
//   ceil(-3.2) = -3.000000000

fn main() {
    // Test abs
    let x: float = -5.5;
    let a: float = abs(x);
    print_float(a);  // 5.500000000
    
    let y: float = 3.14;
    let b: float = abs(y);
    print_float(b);  // 3.140000000
    
    // Test floor
    let p: float = 3.7;
    let q: float = floor(p);
    print_float(q);  // 3.000000000
    
    let r: float = -3.7;
    let s: float = floor(r);
    print_float(s);  // -4.000000000
    
    // Test ceil
    let m: float = 3.2;
    let n: float = ceil(m);
    print_float(n);  // 4.000000000
    
    let u: float = -3.2;
    let v: float = ceil(u);
    print_float(v);  // -3.000000000
}
